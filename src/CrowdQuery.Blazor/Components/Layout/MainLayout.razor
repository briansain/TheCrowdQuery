@inherits LayoutComponentBase

<MudThemeProvider />
<MudPopoverProvider />


<MudLayout>
    <NavMenu />
    <MudMainContent>
        @Body
    </MudMainContent>
</MudLayout>

<div id="blazor-error-ui">
    An unhandled error has occurred.
    <a href="" class="reload">Reload</a>
    <a class="dismiss">🗙</a>
</div>

@code {
    bool _drawerOpen = false;
    private void DrawerToggle()
    {
        _drawerOpen = !_drawerOpen;
    }
}